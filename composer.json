{
  "name": "makeandship/acf-dmd",
  "description": "DMD field types for ACF",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Make and Ship Limited",
      "homepage": "https://www.makeandship.com/"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "specialistpharmacyservice/dmd-client-php",
        "version": "v1.1.0",
        "type": "library",
        "source": {
          "url": "https://github.com/SpecialistPharmacyService/dmd-client-php.git",
          "type": "git",
          "reference": "v1.1.0"
        },
        "require": {
          "composer/installers": "^2.0"
        }
      }
    }
  ],
  "require": {
    "specialistpharmacyservice/dmd-client-php": "v1.1.0",
    "composer/installers": "*",
    "ruflin/elastica": "^7.1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
